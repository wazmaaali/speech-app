      "supportsTablet": true,
      "buildNumber": "4",
      "bundleIdentifier": "TODObundleIdentifier",
      "config": {
        "googleSignIn": {
          "reservedClientId": "TODOreservedClientId"
        }
      },
      "infoPlist":{
       "NSMicrophoneUsageDescription": "The microphone will be used to record your speech samples for research purposes.",
       "UIRequiresFullScreen":true
      }
    },
    "android": {
      "package": "com.speechrecognition.slu",
      "permissions": [
        "RECORD_AUDIO"
      ],
      "googleServicesFile": "/Users/saira/Downloads/google-services.json"
    },
    "description": ""
  }
